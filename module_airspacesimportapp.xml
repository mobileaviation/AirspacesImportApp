<?xml version="1.0" encoding="UTF-8"?>
<project name="module_airspacesimportapp" default="compile.module.airspacesimportapp">
  <dirname property="module.airspacesimportapp.basedir" file="${ant.file.module_airspacesimportapp}"/>
  
  <property name="module.jdk.home.airspacesimportapp" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.airspacesimportapp" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.airspacesimportapp" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.airspacesimportapp" value="-encoding windows-1252 -source 1.7 ${compiler.args}"/>
  
  <property name="airspacesimportapp.output.dir" value="${module.airspacesimportapp.basedir}/out/production/AirspacesImportApp"/>
  <property name="airspacesimportapp.testoutput.dir" value="${module.airspacesimportapp.basedir}/out/test/AirspacesImportApp"/>
  
  <path id="airspacesimportapp.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="airspacesimportapp.module.production.classpath">
    <path refid="${module.jdk.classpath.airspacesimportapp}"/>
    <pathelement location="${basedir}/jts-1.13.jar"/>
    <pathelement location="${basedir}/../../../../Program Files (x86)/PostgreSQL/pgJDBC/postgresql-9.4-1204.jdbc4.jar"/>
    <pathelement location="${basedir}/xmlpull-1.1.3.1.jar"/>
    <pathelement location="${basedir}/kxml2-2.3.0.jar"/>
    <pathelement location="${basedir}/commons-io-2.4.jar"/>
  </path>
  
  <path id="airspacesimportapp.runtime.production.module.classpath">
    <pathelement location="${airspacesimportapp.output.dir}"/>
    <pathelement location="${basedir}/jts-1.13.jar"/>
    <pathelement location="${basedir}/../../../../Program Files (x86)/PostgreSQL/pgJDBC/postgresql-9.4-1204.jdbc4.jar"/>
    <pathelement location="${basedir}/xmlpull-1.1.3.1.jar"/>
    <pathelement location="${basedir}/kxml2-2.3.0.jar"/>
    <pathelement location="${basedir}/commons-io-2.4.jar"/>
  </path>
  
  <path id="airspacesimportapp.module.classpath">
    <path refid="${module.jdk.classpath.airspacesimportapp}"/>
    <pathelement location="${airspacesimportapp.output.dir}"/>
    <pathelement location="${basedir}/jts-1.13.jar"/>
    <pathelement location="${basedir}/../../../../Program Files (x86)/PostgreSQL/pgJDBC/postgresql-9.4-1204.jdbc4.jar"/>
    <pathelement location="${basedir}/xmlpull-1.1.3.1.jar"/>
    <pathelement location="${basedir}/kxml2-2.3.0.jar"/>
    <pathelement location="${basedir}/commons-io-2.4.jar"/>
  </path>
  
  <path id="airspacesimportapp.runtime.module.classpath">
    <pathelement location="${airspacesimportapp.testoutput.dir}"/>
    <pathelement location="${airspacesimportapp.output.dir}"/>
    <pathelement location="${basedir}/jts-1.13.jar"/>
    <pathelement location="${basedir}/../../../../Program Files (x86)/PostgreSQL/pgJDBC/postgresql-9.4-1204.jdbc4.jar"/>
    <pathelement location="${basedir}/xmlpull-1.1.3.1.jar"/>
    <pathelement location="${basedir}/kxml2-2.3.0.jar"/>
    <pathelement location="${basedir}/commons-io-2.4.jar"/>
  </path>
  
  
  <patternset id="excluded.from.module.airspacesimportapp">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.airspacesimportapp">
    <patternset refid="excluded.from.module.airspacesimportapp"/>
  </patternset>
  
  <path id="airspacesimportapp.module.sourcepath">
    <dirset dir="${module.airspacesimportapp.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.airspacesimportapp" depends="compile.module.airspacesimportapp.production,compile.module.airspacesimportapp.tests" description="Compile module AirspacesImportApp"/>
  
  <target name="compile.module.airspacesimportapp.production" depends="register.custom.compilers" description="Compile module AirspacesImportApp; production classes">
    <mkdir dir="${airspacesimportapp.output.dir}"/>
    <javac2 destdir="${airspacesimportapp.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.airspacesimportapp}/javac">
      <compilerarg line="${compiler.args.airspacesimportapp}"/>
      <bootclasspath refid="airspacesimportapp.module.bootclasspath"/>
      <classpath refid="airspacesimportapp.module.production.classpath"/>
      <src refid="airspacesimportapp.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.airspacesimportapp"/>
    </javac2>
    
    <copy todir="${airspacesimportapp.output.dir}">
      <fileset dir="${module.airspacesimportapp.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.airspacesimportapp.tests" depends="register.custom.compilers,compile.module.airspacesimportapp.production" description="compile module AirspacesImportApp; test classes" unless="skip.tests"/>
  
  <target name="clean.module.airspacesimportapp" description="cleanup module">
    <delete dir="${airspacesimportapp.output.dir}"/>
    <delete dir="${airspacesimportapp.testoutput.dir}"/>
  </target>
</project>